{
    "name": "Passmanager",
    "version": "0.0.1",
    "description": "Custom lefuturiste's passmanager",
    "icons": {
      "16": "logo-192.png"
    },
    "background": {
      "persistent": false,
      "scripts": ["background.js"]
    },
    "commands": {
      "passmanager_load_page": {
        "description": "helloword",
        "suggested_key": {
          "default": "Ctrl+Shift+K",
          "mac": "Command+Shift+K"
        }
      },
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+S",
          "mac": "Command+Shift+S"
        }
      }
    },
    "permissions": ["https://*.google.com/", "activeTab", "storage"],
    "browser_action": {
      "default_icon": "logo-192.png",
      "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "content_security_policy":"script-src 'self' https://cdn.jsdelivr.net; object-src 'self'"
 }
